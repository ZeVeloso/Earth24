@page "/"
@namespace Earth24.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;

   
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Earth24</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="Earth24.styles.css" rel="stylesheet" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script type='text/javascript' src='https://www.bing.com/api/maps/mapcontrol?key=Asg3GSasFxgpOxPAnXRZhtQedxn_lxKZ7uKxwxG6TE5bDxHSEdck5FDqCcmXWssE'></script>
    <script src="./ts/BingTsInterop.js"></script>
    <script src='https://www.bing.com/api/maps/mapcontrol?callback=GetMap&key=Asg3GSasFxgpOxPAnXRZhtQedxn_lxKZ7uKxwxG6TE5bDxHSEdck5FDqCcmXWssE' type='text/javascript'></script>

    

</body>
</html>
